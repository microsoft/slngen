<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>$id$</id>
    <version>$version$</version>
    <description>$description$</description>
    <title>$title$</title>
    <authors>$authors$</authors>
    <copyright>$copyright$</copyright>
    <license type="file">LICENSE.txt</license>
    <projectUrl>$projectUrl$</projectUrl>
    <icon>images\Icon.png</icon>
    <developmentDependency>$developmentDependency$</developmentDependency>
    <requireLicenseAcceptance>$requireLicenseAcceptance$</requireLicenseAcceptance>
    <repository type="git" url="$repositoryUrl$" branch="" commit="$repositoryCommit$" />
    <tags>$tags$</tags>
    <packageTypes>
      <packageType name="DotnetTool" />
      <packageType name="Dependency" />
    </packageTypes>
    <dependencies>
      <group targetFramework="net472" />
    </dependencies>
  </metadata>
  <files>
    <file src="$licensefile$" target="" />
    <file src="$icon$" target="images\" />
    <file src="Tasks\Microsoft.VisualStudio.SlnGen.targets" target="build\" />
    <file src="Tasks\Microsoft.VisualStudio.SlnGen.targets" target="buildMultiTargeting\" />
    <file src="bin\$configuration$\net472\**" target="tools\net472" />
  </files>
</package>